module Enumeration {
    type Color = enum { red, green, blue };
    type Other = enum { r, g, b };
    type Other2 = enum { anon_enum_0, anon_enum_1, anon_enum_2, anon_enum_3 };
    type Other3 = enum { anon_enum_4 };
    var color: Color;
    var other: Other;
    var other2: Other2;
    var other3: Other3;
    init {
        color = red;
    }
    next {
        color' = green;
    }
    invariant spec: color != blue;
}
