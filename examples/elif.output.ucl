module Elif {
    var x: real;
    var y: real;

    next {
        if (x == 0.0) {
            y' = 0.0;
        } else {
            if (x == 1.0) {
                y' = 1.0;
            } else {
                if (x == 2.0) {
                    y' = -2.0;
                } else {
                    y' = 0.5;
                }
            }
        }
    }
}