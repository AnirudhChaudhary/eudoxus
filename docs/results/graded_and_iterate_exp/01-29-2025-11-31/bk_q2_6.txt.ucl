module Stack {
  var stack_type: ??;
  var pop: boolean;
  var push: element_type;
  var top: element_type;
  var size: integer;

  init {
    size = 0;

  }
  next {
    top' = (if (size > 0) then stack_type[(size - 1)] else false);
    pop' = (size > 0);
    push' = element_type;
    size' = (if (push) then (size + 1) else (if (pop) then (size - 1) else size));

  }
}
